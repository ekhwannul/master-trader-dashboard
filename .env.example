# Master Trader Dashboard - Environment Variables Template
# Copy this file to .env and fill in your API keys

# ===========================================
# API KEYS (Optional - untuk data real-time)
# ===========================================

# Santiment API (untuk sentiment analysis)
# Get from: https://app.santiment.net/
SANTIMENT_API_KEY=your_santiment_api_key_here

# Whale Alert API (untuk whale tracking)
# Get from: https://whale-alert.io/
WHALE_ALERT_API_KEY=your_whale_alert_api_key_here

# Bybit API (PRIMARY - untuk real-time data)
# Get from: https://www.bybit.com/app/user/api-management
BYBIT_API_KEY=your_bybit_api_key_here
BYBIT_SECRET=your_bybit_secret_here

# Binance API (optional backup)
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET=your_binance_secret_here

# OKX API (optional)
OKX_API_KEY=your_okx_api_key_here
OKX_SECRET=your_okx_secret_here
OKX_PASSPHRASE=your_okx_passphrase_here

# ===========================================
# SERVER CONFIGURATION
# ===========================================

# Port untuk web server
PORT=3000

# Environment mode
NODE_ENV=development

# Exchange default untuk data
EXCHANGE_NAME=bybit

# ===========================================
# TRADING CONFIGURATION
# ===========================================

# Default trading pairs (comma separated)
DEFAULT_SYMBOLS=BTC/USDT,ETH/USDT,BNB/USDT,SOL/USDT,ADA/USDT

# Default timeframe
DEFAULT_TIMEFRAME=1h

# Minimum confidence untuk signals
MIN_CONFIDENCE=85

# Risk/reward ratio minimum
MIN_RISK_REWARD=2.0

# ===========================================
# CACHE CONFIGURATION
# ===========================================

# Cache TTL dalam detik
CACHE_TTL=300

# Redis URL (jika menggunakan Redis)
REDIS_URL=redis://localhost:6379

# ===========================================
# LOGGING CONFIGURATION
# ===========================================

# Log level (error, warn, info, debug)
LOG_LEVEL=info

# Log file path
LOG_FILE=./logs/master-trader.log

# ===========================================
# SECURITY CONFIGURATION
# ===========================================

# JWT Secret (jika implementasi auth)
JWT_SECRET=your_jwt_secret_here

# CORS Origins (comma separated)
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# ===========================================
# WEBHOOK CONFIGURATION (Optional)
# ===========================================

# Discord webhook untuk alerts
DISCORD_WEBHOOK_URL=your_discord_webhook_url_here

# Telegram bot token dan chat ID
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# ===========================================
# ADVANCED SETTINGS
# ===========================================

# Enable/disable features
ENABLE_WHALE_TRACKING=true
ENABLE_SENTIMENT_ANALYSIS=true
ENABLE_FUNDING_RATES=true
ENABLE_AI_PREDICTION=true

# Prediction model settings
PREDICTION_MODEL=ensemble
PREDICTION_LOOKBACK=100
PREDICTION_CONFIDENCE_THRESHOLD=0.85

# Rate limiting
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX_REQUESTS=100