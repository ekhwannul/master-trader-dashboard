# Master Trader Dashboard - Environment Variables
# Copy this file to .env and fill in your actual values

# ===========================================
# API KEYS
# ===========================================

# Bybit API (Get from: https://www.bybit.com/app/user/api-management)
BYBIT_API_KEY=your_bybit_api_key_here
BYBIT_SECRET=your_bybit_secret_here

# Santiment API (Get from: https://app.santiment.net/account)
SANTIMENT_API_KEY=your_santiment_api_key_here

# ===========================================
# SERVER CONFIGURATION
# ===========================================

PORT=3000
NODE_ENV=development

# ===========================================
# TRADING CONFIGURATION
# ===========================================

DEFAULT_SYMBOLS=BTC/USDT,ETH/USDT,BNB/USDT,SOL/USDT
DEFAULT_TIMEFRAME=1h
MIN_CONFIDENCE=85

# ===========================================
# FEATURES
# ===========================================

ENABLE_WHALE_TRACKING=true
ENABLE_SENTIMENT_ANALYSIS=true
ENABLE_FUNDING_RATES=true
ENABLE_AI_PREDICTION=true
